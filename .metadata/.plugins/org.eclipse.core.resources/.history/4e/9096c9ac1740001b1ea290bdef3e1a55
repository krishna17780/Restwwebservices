package com.example.demo;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Component;

@Component
public class BinearySearchImpl {
 
	@Autowired
	private SortAlgorithem sortAlgorithem;

	public BinearySearchImpl(SortAlgorithem sortAlgorithem) {
		super();
		this.sortAlgorithem = sortAlgorithem;
	}



	public int binarySearch(int[] numbers, int numbersToSearchFor) {

		int[] sortedNumbers = sortAlgorithem.sort(numbers);
		
		return 3;

	}

	
}
